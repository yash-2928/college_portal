n  return (\n    <Card className={classes.root}>\n      <CardHeader\n        avatar={\n          <Avatar\n            alt=\"profile image\"\n            //user props\n            src={props.user.fileUrl || UserIcon}\n            className={classes.large}\n            className={classes.avatar}\n          />\n        }\n        action={\n          <IconButton onClick={() => props.delete(props.jobId)}>\n            <Delete />\n          </IconButton>\n        }\n        title={props.user.firstname + props.user.lastname}\n        subheader={date.format(\"DD/MM/YYYY\")}\n      />\n      <CardMedia\n        className={classes.media}\n        //post props\n        image={props.fileUrl}\n      />\n      <CardContent>\n        <Typography variant=\"body2\" color=\"textSecondary\" component=\"p\">\n          {props.companyName}\n          <br />\n          {props.content}\n        </Typography>\n      </CardContent>\n      <CardActions disableSpacing>\n        <IconButton\n          className={clsx(classes.expand, {\n            [classes.expandOpen]: expanded,\n          })}\n          onClick={handleExpandClick}\n          aria-expanded={expanded}\n          aria-label=\"show more\"\n        >\n          <ExpandMoreIcon />\n        </IconButton>\n      </CardActions>\n      <Collapse onClick={() => setShow(!showComments)}>\n        <CardContent>\n          <Typography paragraph>\n            {showComments && (\n              <div>\n                <CreateComment\n                  createComment={(content) =>\n                    props.createComment(props.jobId, content)\n                  }\n                />\n                {props.comments.map((comment, i) => (\n                  <CommentItem key={i} {...comment} />\n                ))}\n              </div>\n            )}\n          </Typography>\n        </CardContent>\n      </Collapse>\n    </Card>\n  );\n}\n","/home/yash29/Projects/portal-forntend/src/component/PostCard.jsx",["156"],"import React from \"react\";\ni